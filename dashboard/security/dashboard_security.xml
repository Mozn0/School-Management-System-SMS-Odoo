<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Admin Group -->
    <record id="group_dashboard_admin" model="res.groups">
        <field name="name">Dashboard Admin</field>
        <field name="category_id" ref="base.module_category_administration"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>


    <record id="dashboard_admin_access" model="ir.model.access">
        <field name="name">dashboard.admin</field>
        <field name="model_id" ref="model_school_report"/>
        <field name="group_id" ref="dashboard.group_dashboard_admin"/>
        <field name="perm_read">1</field>
        <field name="perm_write">1</field>
        <field name="perm_create">1</field>
        <field name="perm_unlink">1</field>
    </record>
</odoo>